    "change-case": "^4.1.2",
    "mkdirp": "^1.0.4",
    "yeoman-generator": "^5.4.2"
  },
  "scripts": {
    "lint": "eslint .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
